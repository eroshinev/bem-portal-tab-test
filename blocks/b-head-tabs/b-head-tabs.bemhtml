block b-head-tabs {
    js: true,
    
    content: {
        var c = {
            block: 'b-menu-horiz',
            mods: { layout: 'normal' },
            content: []
        };
        var ctxContent = this.ctx.content;
	ctxContent.forEach(function(el){
            c.content.push(el);
        });
        return c;
   }
}
